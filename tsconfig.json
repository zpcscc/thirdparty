{
  "extends": "@zpcscc/configs/tsconfig/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "docs/": ["docs"],
      "docs/*": ["docs/*"],
      "src": ["src"],
      "src/*": ["src/*"],
      "@zpcscc/thirdparty": ["src"],
      "@zpcscc/thirdparty/*": ["src/*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*", "docs/**/*", ".eslintrc.js", "babel.config.js"]
}
